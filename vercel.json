{
    "version": 2,
    "builds": [
        {
            "src": "*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "https://peer-stream-backend.vercel.app/"
        },
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ]
}
